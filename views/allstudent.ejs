<%-include('adminheader')%>

<div class="clearfix"></div>
	
  <div class="content-wrapper">
    <div class="container-fluid">
     
      <div class="row mt-3">
        <div class="col-lg-12">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Basic Table</h5>
              <h4 style="color: red;">
                <%if(messages.success){%>
                  <%=messages.suucess%>
                  <%}%>
              </h4> 
			  <div class="table-responsive">
               <table class="table">
                  <thead>
                    <tr>
                      <th scope="col">Sr.No</th>
                      <th scope="col">First</th>
                      <th scope="col">Email</th>
                      <th scope="col">Image</th>
                      <th scope="col">Mobile No.</th>
                      <th scope="col">Action</th>
                    </tr>
                  </thead>
                  <tbody>
                    <%var sr = 1%>
                    <%for(var i of student){%>
                    <tr>
                      <th scope="row"><%=sr++%></th>
                      <td><%=i.name%></td>
                      <td><%=i.email%></td>
                      <td><%=i.img%></td>
                      <td><%=i.mobile%></td>
                      <td>
                        <a href="/admin/delete/<%=i.id%>">Delete</a> ||
                        <!-- <a href="/admin/update/<%=i.id%>">Update</a> || -->
                        <a href="/admin/more/<%=i.id%>" style="color: red;">More</a>
                      </td>
                    </tr>
                    <%}%>
                  </tbody>
                </table>
            </div>
            </div>
          </div>
        </div>
      </div><!--End Row-->
	  
	  <!--start overlay-->
		  <div class="overlay toggle-menu"></div>
		<!--end overlay-->

    </div>
    <!-- End container-fluid-->
    
